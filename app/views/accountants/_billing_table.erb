<!-- locals: -->

<div id="<%= table_type %>" class="margin-bottom">
  <h1 class="border-bottom title"><%= title %></h1>
  <table class="table border-side">
    <thead>
      <tr>
        <th>Clinic Name</th>
        <th>Patient Initials</th>
        <th>Abortion Care $</th>
        <th>Weeks at Proceedure</th>
        <th>Pledge Sent Date</th>
        <th>Check</th>
        <th>Date of Check</th>
      </tr>
    </thead>
    <tbody id="<%= table_type %>_content">
      <% patients.each do |patient| %>
        <tr class="pregnancy-data" id="<%= patient.id %>">
          <td><%= patient.clinic_name %></td>
          <td><%= link_to patient.name, edit_patient_path(patient) %></td>
          <td><%= patient.pregnancy.procedure_cost %></td>
          <td><!-- Weeks at Proceedure --></td>
          <td><!-- Pledge Sent Date --></td>
          <td><!-- Check # --></td>
          <td><!-- Date of Check --></td>
          <td></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
